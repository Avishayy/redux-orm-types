{
  "compilerOptions": {
    "lib": [
      "es2017",
      "esnext"
    ],
    "listFiles": false,
    "listEmittedFiles": false,
    "traceResolution": false,
    "diagnostics": false,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "pretty": true,
    "removeComments": true,
    "stripInternal": true,
    "declaration": true,
    "importHelpers": true,
    "resolveJsonModule": true,
    "noEmit": false,
    "target": "es5",
    "module": "commonjs",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowUnreachableCode": false,
    "outDir": "lib",
    "sourceMap": true,
    "typeRoots": [
      "./node_modules/@types"
    ],
    "baseUrl": "./",
    "paths": {
      "redux-orm": [
        "typings/redux-orm"
      ],
      "redux-orm/*": [
        "typings/redux-orm/*"
      ]
    },
    "rootDir": "."
  },
  "types": [
    "jest"
  ],
  "include": [
    "typings/redux-orm/**/*",
    "test/**/*"
  ]
}
